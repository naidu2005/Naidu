#include<stdio.h>
int main()
{
int a[3][3], i, j;
    printf("\n");
    printf("Enter the entries of 1st matrix");
    for (i=0; i<3; i++)
    {
        for (j=0; j<3; j++)
        {
            printf("\nEnter the element a[%d][%d]: ", i, j);
            scanf("%d", &a[i][j]);
        }
    }
    
for(i=0;i<3;i++){
  for(j=0;j<3;j++){
    printf(" %d",a[j][i]);
  }  
    printf("\n ");
}
}
